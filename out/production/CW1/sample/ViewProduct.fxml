<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewProduct">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../PICS/11.jpg" />
         </image>
      </ImageView>
      <Label opacity="0.66" prefHeight="401.0" prefWidth="602.0" style="-fx-background-color: #1c2833;" />
      <TableView fx:id="productTblView" layoutX="85.0" layoutY="55.0" opacity="0.76" prefHeight="290.0" prefWidth="388.0" style="-fx-background-color: #000000;">
        <columns>
          <TableColumn fx:id="productIDTblColumn" prefWidth="136.0" text="Product ID" />
            <TableColumn fx:id="productNameTblColumn" prefWidth="143.0" text="Product Name" />
          <TableColumn fx:id="categoryTblColumn" prefWidth="108.0" text="Category Name" />
        </columns>
      </TableView>
      <Button layoutX="430.0" layoutY="345.0" mnemonicParsing="false" onAction="#methodBack" style="-fx-background-color: #000000;" text="Back" textFill="#e4e0e0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow color="#b90f0f" />
         </effect></Button>
   </children>
</AnchorPane>
