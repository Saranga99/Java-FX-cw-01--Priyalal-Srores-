<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EditCategory">
   <children>
      <ImageView fitHeight="507.0" fitWidth="618.0" layoutX="-7.0" layoutY="-53.0" pickOnBounds="true">
         <image>
            <Image url="@../PICS/Coffee%20types.jpg" />
         </image>
      </ImageView>
      <Label layoutX="-5.0" layoutY="-3.0" opacity="0.52" prefHeight="461.0" prefWidth="618.0" style="-fx-background-color: #000000;" />
      <Label layoutX="98.0" layoutY="52.0" opacity="0.76" prefHeight="262.0" prefWidth="350.0" style="-fx-background-color: #1c2833;" />
      <Button layoutX="247.0" layoutY="245.0" mnemonicParsing="false" onAction="#clickBtnUpdate" style="-fx-background-color: #000000;" text="Update" textFill="#f8eded">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow color="#16fce9" />
         </effect></Button>
      <TextField fx:id="txtExistingCategory" layoutX="191.0" layoutY="103.0" prefHeight="25.0" prefWidth="167.0" promptText="Enter Existing Category Here" style="-fx-background-color: #000000;">
         <effect>
            <DropShadow color="#084a6b" />
         </effect></TextField>
      <TextField fx:id="txtNewCategory" layoutX="191.0" layoutY="175.0" prefHeight="25.0" prefWidth="167.0" promptText="Enter New Category Here" style="-fx-background-color: #000000;">
         <effect>
            <DropShadow color="#084a6b" />
         </effect></TextField>
      <Button layoutX="398.0" layoutY="281.0" mnemonicParsing="false" onAction="#methodBack" style="-fx-background-color: #000000;" text="Back" textFill="#e8e3e3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow color="#b90f0f" />
         </effect></Button>
   </children>
</AnchorPane>
